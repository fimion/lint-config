{
  "name": "@fimion/lint-config",
  "version": "1.0.1",
  "description": "My Personal Linting Configs",
  "type": "module",
  "main": "index.js",
  "exports": {
    "./prettier": {
      "default": "./prettier.config.js"
    },
    "./eslint": {
      "default": "./eslint.config.js"
    },
    "./eslint-ts": {
      "default": "./eslint-ts.config.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint '**/*.{js,json,vue}'",
    "eslint:fix": "eslint --fix '**/*.{js,json,vue}'",
    "prettier": "prettier -c '**/*.{js,json,vue}'",
    "prettier:fix": "prettier -w '**/*.{js,json,vue}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fimion/lint-config.git"
  },
  "keywords": [
    "linting",
    "js"
  ],
  "author": "Alex Riviere",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fimion/lint-config/issues"
  },
  "homepage": "https://github.com/fimion/lint-config#readme",
  "devDependencies": {
    "@types/eslint__js": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "prettier": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest"
  },
  "peerDependencies": {
    "@eslint/js": ">= 9",
    "eslint": ">= 9",
    "eslint-config-prettier": ">= 9",
    "eslint-plugin-prettier": ">= 5",
    "eslint-plugin-vue": ">= 9",
    "prettier": ">= 3",
    "typescript-eslint": ">=8.0.0-alpha.29"
  }
}
